        {eper, ".*", {git, "git://github.com/basho/eper.git", {tag, "0.61-2-gfdfd63c"}}},
        {sext, ".*", {git, "git://github.com/esl/sext", {tag, "0.4.1"}}},
        {druuid, ".*", {git, "git://github.com/kellymclaughlin/druuid.git", {tag, "70a5b0475"}}},
